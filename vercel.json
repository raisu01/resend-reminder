{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "client/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.js"
      },
      {
        "src": "/dashboard/api/requests",
        "dest": "/api/dashboard.js"
      },
      {
        "src": "/send",
        "dest": "/api/send.js"
      },
      {
        "handle": "filesystem"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/client/dist/assets/$1"
      },
      {
        "src": "/favicon.ico",
        "dest": "/client/dist/favicon.ico"
      },
      {
        "src": "/(.*)",
        "dest": "/client/dist/index.html"
      }
    ]
  }
  